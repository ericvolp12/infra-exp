{
    "pools": {
        "primary": {
            "servers": [
                "memcached4:11211"
            ]
        },
        "replica1": {
            "servers": [
                "memcached5:11211"
            ]
        },
        "replica2": {
            "servers": [
                "memcached1:11211"
            ]
        },
        "replica3": {
            "servers": [
                "memcached2:11211"
            ]
        },
        "replica4": {
            "servers": [
                "memcached3:11211"
            ]
        }
    },
    "named_handles": [
        {
            "type": "FailoverRoute",
            "name": "replica_failover",
            "children": [
                {
                    "type": "PoolRoute",
                    "pool": "replica1"
                },
                {
                    "type": "PoolRoute",
                    "pool": "replica2"
                },
                {
                    "type": "PoolRoute",
                    "pool": "replica3"
                },
                {
                    "type": "PoolRoute",
                    "pool": "replica4"
                }
            ]
        },
        {
            "type": "PoolRoute",
            "name": "primary_warmup",
            "pool": "primary",
            "slow_warmup": {
                "failoverTarget": "replica_failover",
                "settings": {
                    "enable_threshold": 0.5,
                    "disable_threshold": 0.9,
                    "start": 0.99
                }
            }
        },
        {
            "type": "AllSyncRoute",
            "name": "write_all",
            "children": [
                {
                    "type": "PoolRoute",
                    "pool": "primary"
                },
                {
                    "type": "PoolRoute",
                    "pool": "replica1"
                },
                {
                    "type": "PoolRoute",
                    "pool": "replica2"
                },
                {
                    "type": "PoolRoute",
                    "pool": "replica3"
                },
                {
                    "type": "PoolRoute",
                    "pool": "replica4"
                }
            ]
        }
    ],
    "route": {
        "type": "OperationSelectorRoute",
        "operation_policies": {
            "get": {
                "type": "FailoverRoute",
                "children": [
                    "primary_warmup",
                    "replica_failover"
                ]
            },
            "gets": {
                "type": "FailoverRoute",
                "children": [
                    "primary_warmup",
                    "replica_failover"
                ]
            },
            "set": "write_all",
            "delete": "write_all"
        }
    }
}
