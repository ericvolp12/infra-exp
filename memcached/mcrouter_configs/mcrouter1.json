{
    "pools": {
        "primary": {
            "servers": [
                "memcached1:11211"
            ]
        },
        "replicas": {
            "servers": [
                "memcached2:11211",
                "memcached3:11211",
                "memcached4:11211",
                "memcached5:11211"
            ]
        }
    },
    "route": {
        "type": "OperationSelectorRoute",
        "operation_policies": {
            "get": {
                "type": "FailoverRoute",
                "children": [
                    {
                        "type": "WarmUpRoute",
                        "cold": "PoolRoute|primary",
                        "warm": "PoolRoute|replicas"
                    },
                    {
                        "type": "PoolRoute",
                        "pool": "replicas"
                    }
                ]
            },
            "gets": {
                "type": "FailoverRoute",
                "children": [
                    {
                        "type": "WarmUpRoute",
                        "cold": "PoolRoute|primary",
                        "warm": "PoolRoute|replicas"
                    },
                    {
                        "type": "PoolRoute",
                        "pool": "replicas"
                    }
                ]
            },
            "set": {
                "type": "AllSyncRoute",
                "children": [
                    {
                        "type": "PoolRoute",
                        "pool": "primary"
                    },
                    {
                        "type": "AllSyncRoute",
                        "children": "PoolRoute|replicas"
                    }
                ]
            },
            "delete": {
                "type": "AllSyncRoute",
                "children": [
                    {
                        "type": "PoolRoute",
                        "pool": "primary"
                    },
                    {
                        "type": "AllSyncRoute",
                        "children": "PoolRoute|replicas"
                    }
                ]
            }
        }
    }
}
